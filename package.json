      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.5",
    "nuxt": "^3.13.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@fancyapps/ui": "^5.0.36",
    "@morev/vue-transitions": "^3.0.2",
    "@nuxtjs/svg-sprite": "^1.0.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@types/sequelize": "^4.28.20",
    "@types/swiper": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-vue": "^9.29.0",
    "express": "^4.21.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nuxt-swiper": "^1.2.2",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.80.3",
    "typescript": "^5.6.3"
